{
  "name": "@vendemas/root",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "echo 'TypeScript checking will be available when projects are created'",
    "prepare": "husky install",
    "verify": "npm run lint && npm run format:check",
    "clean": "nx reset && rm -rf .eslintcache",
    "build": "echo 'Build will be available when projects are created'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "affected:build": "echo 'Affected builds will be available when projects are created'",
    "affected:test": "echo 'Affected tests will be available when projects are created'",
    "affected:lint": "echo 'Affected linting will be available when projects are created'"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "9.33.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/devkit": "21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/next": "21.3.11",
    "@nx/playwright": "21.3.11",
    "@nx/vite": "21.3.11",
    "@playwright/test": "^1.48.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.7.0",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "18.16.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "babel-jest": "^30.0.2",
    "eslint": "9.33.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "9.1.7",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.5",
    "nx": "21.3.11",
    "playwright": "^1.48.0",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.39.1",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}
