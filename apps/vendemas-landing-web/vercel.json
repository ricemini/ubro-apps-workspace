{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npx nx run vendemas-landing-web:build:production",
  "devCommand": "npx nx run vendemas-landing-web:serve --port=3000",
  "installCommand": "pnpm install",
  "outputDirectory": "dist/vendemas-landing-web/.next",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ]
}
