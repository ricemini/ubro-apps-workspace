{
  "name": "vendemas-landing-web",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/vendemas-landing-web/src",
  "tags": [
    "scope:app",
    "type:app",
    "platform:web",
    "role:landing",
    "framework:nextjs"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [".next"],
      "defaultConfiguration": "production",
      "options": {
        "command": "npm run build",
        "cwd": "apps/vendemas-landing-web"
      },
      "configurations": {
        "development": {
          "command": "npm run build",
          "cwd": "apps/vendemas-landing-web"
        },
        "production": {
          "command": "npm run build",
          "cwd": "apps/vendemas-landing-web"
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/vendemas-landing-web"
      },
      "configurations": {
        "development": {
          "command": "npm run dev",
          "cwd": "apps/vendemas-landing-web"
        },
        "production": {
          "command": "npm run start",
          "cwd": "apps/vendemas-landing-web"
        }
      }
    },
    "export": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build && npm run export",
        "cwd": "apps/vendemas-landing-web"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "apps/vendemas-landing-web"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["../coverage/vendemas-landing-web"],
      "options": {
        "command": "npm run test",
        "cwd": "apps/vendemas-landing-web"
      }
    }
  }
}
