{
  "name": "vendemas-negocio-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/vendemas-negocio-web/src",
  "tags": [
    "scope:app",
    "type:app",
    "platform:web",
    "role:negocio",
    "framework:angular"
  ],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/vendemas-negocio-web",
        "browser": "apps/vendemas-negocio-web/src/main.ts",
        "tsConfig": "apps/vendemas-negocio-web/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/vendemas-negocio-web/public"
          },
          {
            "glob": "index.html",
            "input": "apps/vendemas-negocio-web/src",
            "output": "."
          }
        ],
        "styles": ["apps/vendemas-negocio-web/src/styles.scss"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "vendemas-negocio-web:build:production"
        },
        "development": {
          "buildTarget": "vendemas-negocio-web:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "vendemas-negocio-web:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../coverage/vendemas-negocio-web"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "vendemas-negocio-web:build",
        "port": 4200,
        "staticFilePath": "dist/vendemas-negocio-web/browser",
        "spa": true
      }
    }
  }
}
